<nav aria-label="breadcrumb"></nav>
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/">Home</a></li>
    <li class="breadcrumb-item"><a href="/explore-all-category">Category</a></li>
    <li class="breadcrumb-item active" aria-current="page">Explore Categories</li>
  </ol>
</nav>

<section class="pb-4 pt-4">
    <div class="d-flex mb-2 align-items-center">
        <h2>{{categoryid}} Recipes</h2>
        {{!-- <a href="/explore-latest" class="ms-auto">View More</a> --}}
    </div>

    <div class="row row-cols-2 row-cols-lg-5 g-2 g-lg-3">

        {{#each recipe}}
        <a href="/recipe/{{this._id}}" class="col text-center category__link">
            <div class="category__img latest__img shadow">
                <img src="/uploads/recipes/{{this.name}}.png" alt="Indian Food" loading="lazy" style="height:100%">
            </div>
            <div class="pt-1">{{this.name}}</div>
        </a>
        {{/each}}

    </div>
</section>